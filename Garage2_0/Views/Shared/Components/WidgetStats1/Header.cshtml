@model GarageStatistics

<div class="bg-dark">
    <div class="container">
        <ul id="div1" class="list-group list-group-horizontal small" style="min-height:40px">
            @foreach (var s in Model.GetType().GetProperties())
            {
                <li class="list-group-item text-light bg-dark flex-fill d-flex justify-content-between align-items-center">
                    <div>@Html.DisplayName(s.Name)</div>
                    <span class="badge bg-secondary text-light rounded-pill">@Html.Display(s.Name)</span>
                </li>
            }
        </ul>
    </div>
</div>