@model GarageStatistics

    <div class="container">
    <ul id="div1" class="list-group small border-0 p-0">
        <li class="list-group-item border-0 fw-bold p-0">Garage Statistics</li>
            @foreach (var s in Model.GetType().GetProperties())
            {
            <li class="list-group-item flex-fill d-flex justify-content-between align-items-center border-0 p-0">
                    <div>@Html.DisplayName(s.Name)</div>
                    <span class="badge bg-secondary text-light rounded-pill">@Html.Display(s.Name)</span>
                </li>
            }
        </ul>
    </div>

